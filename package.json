  "devDependencies": {
    "@humeai/eslint-config": "workspace:*",
    "@humeai/typescript-config": "workspace:*",
    "prettier": "^3.1.0",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  }
}
