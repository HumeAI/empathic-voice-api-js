{
  "name": "voice-sdk",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:packages": "turbo build --filter='./packages/**'",
    "dev": "turbo dev",
    "dev:iframe": "turbo run dev --filter='./packages/**' --filter=example-vite-app-embed",
    "lint": "turbo lint",
    "test": "turbo test",
    "test-watch": "turbo test-watch",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "release": "pnpm install && pnpm build:packages && pnpm publish -r --access public --no-git-checks --tag beta"
  },
  "devDependencies": {
    "@humeai/eslint-config": "workspace:*",
    "@humeai/typescript-config": "workspace:*",
    "prettier": "^3.2.5",
    "turbo": "^1.13.4"
  },
  "packageManager": "pnpm@9.1.4",
  "engines": {
    "node": ">=18"
  }
}
